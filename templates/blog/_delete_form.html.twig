<form method="post" action="{{ path('app_blog_delete', {'id': article.id}) }}" onsubmit="return confirm('Voulez vous vraiment supprimer cet article ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ article.id) }}">
	<button class="btn btn-outline-danger ms-1 mt-1">supprimer</button>
</form>
